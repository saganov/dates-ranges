{
    "name": "saganov/dates-ranges",
    "description": "Dates Ranges",
    "keywords": ["saganov", "date", "range", "test"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1 || ^7.2",
        "robmorgan/phinx": "^0.10.8",
        "symfony/http-foundation": "^4.3",
        "symfony/routing": "^4.3"
    },
    "require-dev": {
        "codeception/codeception": "^3.0",
        "flow/jsonpath": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "DateRange\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/_support"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.17"
        }
    }
}
